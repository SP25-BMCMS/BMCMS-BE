{
  "name": "building-maintenace-ai-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/building-maintenace-ai-backend/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma:generate": "npx prisma generate --schema=apps/users/prisma/schema.prisma && npx prisma generate --schema=apps/tasks/prisma/schema.prisma && npx prisma generate --schema=apps/cracks/prisma/schema.prisma && npx prisma generate --schema=apps/buildings/prisma/schema.prisma && npx prisma generate --schema=apps/schedules/prisma/schema.prisma && npx prisma generate --schema=apps/chatbot/prisma/schema.prisma",
    "prisma:deploy": "npx prisma migrate dev --schema=apps/users/prisma/schema.prisma --name init && npx prisma migrate dev --schema=apps/tasks/prisma/schema.prisma --name init && npx prisma migrate dev --schema=apps/cracks/prisma/schema.prisma --name init && npx prisma migrate dev --schema=apps/buildings/prisma/schema.prisma --name init && npx prisma migrate dev --schema=apps/schedules/prisma/schema.prisma --name init && npx prisma migrate dev --schema=apps/chatbot/prisma/schema.prisma --name init",
    "prisma:seed:tasks": "ts-node apps/Tasks/prisma/seed.ts",
    "prisma:seed:schedules": "ts-node apps/schedules/prisma/seed.ts",
    "prisma:seed:users": "ts-node apps/users/prisma/seed.ts",
    "prisma:seed:buildings": "ts-node apps/buildings/prisma/seed.ts",
    "nest:start": "concurrently \"nest start buildings --watch\" \"nest start tasks --watch\" \"nest start schedules --watch\" \"nest start users --watch\" \"nest start cracks --watch\" \"nest start chatbot --watch\" \"nest start notifications --watch\"",
    "rebuild": "rimraf dist && npm run prisma:generate && npm run build"
  },

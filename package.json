    "@nestjs/swagger": "^11.0.6",
    "@nestjs/websockets": "^11.0.13",
    "@prisma/adapter-neon": "^6.6.0",
    "@prisma/client": "^6.6.0",
    "@prisma/extension-accelerate": "^1.3.0",
    "@prisma/extension-optimize": "^1.1.8",
